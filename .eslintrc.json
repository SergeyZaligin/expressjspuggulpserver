{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "AppError": true
    },
    "extends": ["eslint:recommended"],
    "rules": {
        "no-console": 0,
        "object-curly-spacing": 0,
        "max-len": 0,
        "require-jsdoc": 0,
        "no-invalid-this": 0
    }
}